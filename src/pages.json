        "selectedIconPath": "static/icons/registration-icon-true.png"
      },
      {
        "pagePath": "pages/interview/interview",
        "text": "面试",
        "iconPath": "static/icons/interview-icon-false.png",
        "selectedIconPath": "static/icons/interview-icon-true.png"
      }
    ]
  }
